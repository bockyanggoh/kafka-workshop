<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
    <metadata>
        <id>CAKafka</id>
        <version>1.0.1</version>
        <authors>bockyanggoh</authors>
        <language>en-US</language>
        <license type="expression">MIT</license>
        <requireLicenseAcceptance>true</requireLicenseAcceptance>
        <description>
            A simple Kafka library for DotNetCore 3.1. Provides wrapper classes for BackgroundService consumer, KafkaProducer and normal KafkaConsumer.
        </description>
        <dependencies>
            <dependency id="Confluent.Kafka" version="1.3.0" />
            <dependency id="Confluent.Apache.Avro" version="1.7.7.7" />
            <dependency id="Confluent.Apache.SchemaRegistry.Serdes" version="1.3.0" />
            <dependency id="Confluent.Apache.SchemaRegistry" version="1.3.0" />
        </dependencies>
        <frameworkAssemblies>
            <frameworkAssembly assemblyName="Microsoft.NETCore.App" targetFramework="netcoreapp3.1"/>
        </frameworkAssemblies>
    </metadata>
</package>